# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Application
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Service Implementation Selection
# true: Spring Boot API 사용 (프로덕션)
# false: Next.js API 사용 (로컬 개발)
NEXT_PUBLIC_USE_SPRING_BOOT=false

# Spring Boot API URL (USE_SPRING_BOOT=true일 때만 필요)
NEXT_PUBLIC_SPRING_API_URL=http://localhost:8080

# Environment
NODE_ENV=development